  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "puppeteer": "^3.3.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.11.0",
    "react-loading-overlay": "^1.0.1",
    "react-router-dom": "^5.2.0",
    "simple-youtube-api": "^5.2.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  },
  "engines": {
    "node": "12.16.2"
  }
